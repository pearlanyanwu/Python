# 1) create a FROZENSET with at least 4 elements.
myFrozenSet = (["Cat", "Dog", "Frog", "Fish"])
print(myFrozenSet)

# 2) add an element.
myFrozenSet.append("Snake")
print(myFrozenSet)

# 3) remove an element.
print(myFrozenSet)

# 4) insert an element somewhere in the middle.
myFrozenSet.insert(3, "Hamster")
print(myFrozenSet)

# 5) add another array of the same data type.
myFrozenSet2 = (["Turtle","Pig", "Horse", "Goat"])
myFrozenSet.append(myFrozenSet2)
print(myFrozenSet)

# 6) add another array of a different data type.
myFrozenSet.append(aTuple)
print(myFrozenSet)

# 7) create a range of negative numbers.
negFrozenSet = frozenset(range(-100, -301, -50))
print(negFrozenSet)

# 8) create a range of positive numbers.
posFrozenSet = frozenset(range(100, 301, 50))
print(posFrozenSet)

# 9) insert a range, if allowed.
myFrozenSet.insert(5, posFrozenSet)
print(myFrozenSet)

### NOTE: frozensets can only be elongated, not shortened. ###
